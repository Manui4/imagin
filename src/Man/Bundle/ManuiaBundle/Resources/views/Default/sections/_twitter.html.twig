<div class="l-container">
    <article class="article">
        <div class="row">
            <div class="col-s-12">
                <hgroup>
	                <h1><span>On twitte ?</span></h1>
	                <h2><a class="twitter-timeline" href="https://twitter.com/antoine_michael">Tweets de @antoine_michael</a></h2>
                </hgroup>
            </div>
            <div class="col-s-2">
                <img src="{{ asset("/bundles/manmanuia/images/parallax/twitter-share.png") }}"
                    style="width: 150px; height: 150px; opacity:1;"
                    data-bottom-top="top: -200px; left:500px;transform:rotate(0deg);"
                    data-top-bottom="top: 2000px; transform:rotate(360deg); left:0px;"
                    data-anchor-target="#hash-twitter"
                    class="parallax-img-twitter" >
                <img src="{{ asset("/bundles/manmanuia/images/parallax/twitter-share.png") }}"
                    style="width: 100px; height: 100px; opacity:0.6;"
                    data-bottom-top="top: -500px; left:0px;"
                    data-top-bottom="top: 1500px; left:300px;"
                    data-anchor-target="#hash-twitter"
                    class="parallax-img-twitter" >
                <img src="{{ asset("/bundles/manmanuia/images/parallax/twitter-share.png") }}"
                    style="width: 70px; height: 70px; opacity:1;"
                    data-bottom-top="top: 1000px;left:50px;"
                    data-top-bottom="top: -100px; left:400px;"
                    data-anchor-target="#hash-twitter"
                    class="parallax-img-twitter" >
                <img src="{{ asset("/bundles/manmanuia/images/parallax/twitter-share.png") }}"
                    style="width: 50px; height: 50px; opacity:0.3;"
                    data-bottom-top="top: 500px; left:0px;"
                    data-top-bottom="top: 0px; left:300px;"
                    data-anchor-target="#hash-twitter"
                    class="parallax-img-twitter" >
            </div>
            <div class="col-s-5">
                <div class="center-block">
                    {{ render(controller('ManManuiaBundle:Block:lastTweets')) }}
                </div>
            </div>
        </div>
    </article>
</div>

{# data-bottom-top="transform: translate3d(0px, -100%, 0px);"#}
