<div class="l-container">
    <article class="article">
        <div class="row">
            <div class="col-s-12 col-m-3">
                <div class="row">
                    <div class="col-s-12">
                        <h2><span>Qui je suis ?</span></h2>
                    </div>
                    <div class="col-s-12">
                        <p style="text-align: center;"><span>Co-Founder &ndash; Chief Executive Officer</span></p>
                    </div>
                    <div class="col-s-12">
                        <div class="headshots aligncenter">
                            <div class="list-headshots-top">
                                <div class="list-items">
                                    {% for key, image in images %}
                                    <div class="item"  style="left:{{ key * 125 }}px">
                                        <img alt="" src="{{ image }}"/>
                                    </div>
                                    {% endfor %}
                                </div>
                            </div>
                            <div class="list-headshots-bottom">
                                <div class="list-items">
