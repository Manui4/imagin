{% extends "ManManuiaBundle:Template:layout.html.twig" %}
{% import 'ManManuiaBundle:Template:helpers/_menu.html.twig' as menu %}

{% block header %}
    {% set manuia_color_logo = 'orange' %}
    {{ parent() }}
{% endblock %}

{% block content_header_more %}
    {{ parent() }}

{% endblock %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" type="text/css" href="{{ asset("/bundles/manphotography/js/owl/assets/owl.carousel.css") }}">
    <link rel="stylesheet" type="text/css" href="{{ asset("/bundles/manphotography/css/animate.css") }}">
    <link rel="stylesheet" type="text/css" href="{{ asset("/bundles/manphotography/css/styles.css") }}">
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    {% javascripts
        '@ManPhotographyBundle/Resources/public/js/owl/owl.carousel.min.js'
        '@ManPhotographyBundle/Resources/public/js/masonry/jquery.masonry.min.js'
    %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script src="/bundles/manphotography/js/photography-app.js"></script>
{% endblock %}